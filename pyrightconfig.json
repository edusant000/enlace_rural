{
    "include": [
        "src"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "src/experimental",
        "src/typestubs"
    ],
    "ignore": [
        "src/oldstuff"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "stubPath": "src/stubs",
    "venvPath": ".",
    "pythonVersion": "3.9",
    "pythonPlatform": "Darwin",
    "extraPaths": [
        "src"
    ],
    "executionEnvironments": [
        {
            "root": "src",
            "extraPaths": ["src"]
        }
    ]
}